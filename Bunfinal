for invoice, rule_index in matching_invoices:
    description = rule_descriptions.get(rule_index + 1, None)
    output_row = {
        column: T5[column] if column in T5 else invoice[column] for column in output_columns
    }
    if rule_index is not None and description is not None:  # Check if both rule_index and description are not None
        output_row['Match Rule'] = rule_index + 1
        output_row['Description'] = description
    else:
        output_row['Match Rule'] = None
        output_row['Description'] = None
    output_rows.append(output_row)
